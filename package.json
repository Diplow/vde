{
  "name": "hexframe",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=./config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./config/drizzle.config.ts",
    "db:populate": "tsx scripts/populate-db.ts",
    "db:push": "drizzle-kit push --config=./config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./config/drizzle.config.ts",
    "db:create-placeholder-user": "dotenv -e .env -e .env.local -- tsx scripts/create-placeholder-user.ts",
    "db:delete": "tsx scripts/delete-database.ts",
    "dev": "next dev",
    "format:check": "prettier --config ./config/prettier.config.js --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --config ./config/prettier.config.js --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "init-db": "tsx scripts/init-db.ts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "./scripts/run-tests.sh",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:profile": "vitest run --logHeapUsage",
    "test:e2e": "./scripts/run-e2e-with-seed.sh",
    "test:e2e:ui": "./scripts/run-e2e-with-seed.sh --ui",
    "test:e2e:debug": "./scripts/run-e2e-with-seed.sh --debug",
    "test:e2e:headed": "./scripts/run-e2e-with-seed.sh --headed",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:setup": "./scripts/setup-e2e-tests.sh",
    "test:e2e:seed": "NODE_ENV=test SKIP_ENV_VALIDATION=true DATABASE_URL=\"$TEST_DATABASE_URL\" MISTRAL_API_KEY=whatever YOUTUBE_API_KEY=whatever npx tsx tests/e2e/fixtures/seed-test-data.ts",
    "test:e2e:noseed": "playwright test --config=./config/playwright.config.ts",
    "test:e2e:ci": "E2E_MODE=ci ./scripts/run-e2e-with-seed.sh",
    "test:e2e:ci:run": "./scripts/run-e2e-tests.sh",
    "youtube": "node scripts/youtube.js",
    "youtube:transcript": "tsx src/lib/youtube/get-transcript.ts transcript",
    "youtube:analyze": "tsx src/lib/youtube/get-transcript.ts analyze",
    "map:build": "dotenv -e .env -e .env.local -- tsx scripts/buildMap.ts",
    "map:build:js": "dotenv -e .env -e .env.local -- node scripts/buildMap.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "mcp:build": "esbuild src/app/mcp/server.ts --bundle --platform=node --target=node20 --outfile=dist/mcp-server.js --external:@modelcontextprotocol/sdk --external:zod --format=esm",
    "mcp:start": "node dist/mcp-server.js",
    "mcp:dev": "tsx src/app/mcp/server.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.12.3",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
    "@mistralai/mistralai": "^1.5.2",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.77.2",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "@vercel/analytics": "^1.5.0",
    "better-auth": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.4.2",
    "lucide-react": "^0.481.0",
    "next": "^15.2.4",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-markdown": "^10.1.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.3",
    "superjson": "^2.2.1",
    "svix": "^1.61.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@mdx-js/react": "^3.1.0",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/experimental-nextjs-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "3.0.7",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "^3.0.7",
    "dotenv-cli": "^7.0.0",
    "drizzle-kit": "^0.24.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-storybook": "^0.11.3",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "^8.6.14",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.20.2",
    "typescript": "^5.5.3",
    "vite": "6.3.5",
    "vitest": "^3.0.7"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "pnpm@8.15.5"
}
